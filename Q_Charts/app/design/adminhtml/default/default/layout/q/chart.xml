<?xml version="1.0" encoding="UTF-8"?>
<layout>
    <default>
        <reference name="head">
            <action method="addJs"><script>q/admin/Chart.js</script></action>
            <action method="addJs"><script>lib/jquery/jquery-1.10.2.min.js</script></action>
            <action method="addJs"><script>q/admin/noconflict.js</script></action>
            <action method="addCss"><stylesheet>q/admin/legend.css</stylesheet></action>
            <block type="core/html_calendar" name="html_calendar" as="html_calendar" template="page/js/calendar.phtml"/>
            <action method="setTemplate">
                <template>q_admin/page/head.phtml</template>
            </action>
        </reference>
    </default>
    <adminhtml_dashboard_index>
        <reference name="head">
            <action method="addJs"><stylesheet>q/admin/googleLoader.js</stylesheet></action>
        </reference>
    </adminhtml_dashboard_index>

    <adminhtml_bestsellers_index>
        <reference name="messages">
            <action method="addNotice" translate="message">
                <message>This report depends on timezone configuration. Once timezone is changed, the lifetime statistics need to be refreshed.</message>
            </action>
        </reference>

        <reference name="content">
            <block type="q_admin/adminhtml_bestsellers_form" name="bestseller_form" template="q_admin/bestsellers/form.phtml">
                <block type="q_admin/adminhtml_bestsellers_tab_graph" name="bestseller_graph" as="bestseller_graph" template="q_admin/bestsellers/diagram.phtml" />
                <block type="q_admin/adminhtml_bestsellers_tab_table" name="bestseller_table" as="bestseller_table" template="q_admin/bestsellers/table.phtml" />
            </block>
        </reference>
    </adminhtml_bestsellers_index>

